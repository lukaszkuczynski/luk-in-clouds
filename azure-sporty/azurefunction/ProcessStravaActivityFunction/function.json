{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "blob",
      "direction": "in",
      "name": "tokenIn",
      "dataType": "string",
      "path": "tokens/tokenstrava.json",
      "connection": "AzureWebJobsStorage"
    },    
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "tokenOut",
      "dataType": "string",
      "path": "tokens/tokenstrava.json",
      "connection": "AzureWebJobsStorage"
    }
  ]
}